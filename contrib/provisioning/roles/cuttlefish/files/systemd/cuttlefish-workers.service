[Unit]
Description=Cuttlefish Workers
After=network.target
Documentation=

[Service]
User=cuttlefish
#EnvironmentFile=/home/cuttlefish/env
WorkingDirectory=/home/cuttlefish
ExecStart=/usr/bin/bundle exec rake sidekiq -e production
KillMode=process
Restart=on-failure
CPUAccounting=true

[Install]
WantedBy=multi-user.target
